{
    "name": "pctux/yii2-invisible-recaptcha",
    "description": "Google Invisible reCaptcha for Yii2",
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/pctux/yii2-invisible-recaptcha",
        "issues": "https://github.com/pctux/yii2-invisible-recaptcha/issues"
    },
    "authors": [
        {
            "name": "szaboolcs",
            "homepage": "https://github.com/szaboolcs"
        },
        {
            "name": "pctux",
            "homepage": "https://github.com/pctux"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "source": {
        "url": "git@github.com:pctux/yii2-invisible-recaptcha.git",
        "type": "git",
        "reference": "master"
    },
    "keywords": [
        "recaptcha",
        "yii2"
    ],
    "autoload": {
        "psr-4": {
            "pctux\\recaptcha\\": ""
        }
    }
}